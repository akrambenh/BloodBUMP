<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterAdminActivity">
    <Button
        android:id="@+id/button2"
        android:layout_width="81dp"
        android:layout_height="32dp"
        android:background="@drawable/signup_button_switch"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="switch_login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.292"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053"
        app:srcCompat="@drawable/login_switch_title"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    <EditText
        android:id="@+id/venue_name"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:background="@drawable/input_field"
        android:ems="10"
        android:hint="Medical Institution Name"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48sp"
        android:textColorHint="@color/light_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.169" />

    <EditText
        android:id="@+id/pseudo"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:background="@drawable/input_field"
        android:ems="10"
        android:hint="Supervisor Pseudo"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48sp"
        android:textColorHint="@color/light_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.409" />

    <EditText
        android:id="@+id/venue_email"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:background="@drawable/input_field"
        android:ems="10"
        android:hint="Email"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48sp"
        android:textColorHint="@color/light_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.288" />

    <EditText
        android:id="@+id/venue_password"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:background="@drawable/input_field"
        android:ems="10"
        android:hint="Password"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48sp"
        android:textColorHint="@color/light_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625" />

    <EditText
        android:id="@+id/venue_ConfPassword"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:background="@drawable/input_field"
        android:ems="10"
        android:hint="Confirm Password"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:minHeight="48sp"
        android:textColorHint="@color/light_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745" />

    <Button
        android:id="@+id/addVenue_button"
        android:layout_width="307dp"
        android:layout_height="56dp"
        android:background="@drawable/sign_up_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.902"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/venue_phone"
        android:layout_width="250dp"
        android:layout_height="48dp"
        android:hint="Phone"
        android:textColorHint="@color/light_red"
        android:ems="10"
        android:background="@drawable/input_field"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>